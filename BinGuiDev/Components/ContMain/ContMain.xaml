<UserControl x:Class="BinGuiDev.Components.ContMain.ContMain"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:local="clr-namespace:BinGuiDev.Components.ContMain"
             xmlns:contDateTime="clr-namespace:BinGuiDev.Components.ContDateTime"
             xmlns:contChart="clr-namespace:BinGuiDev.Components.ContChart"
             mc:Ignorable="d"
             d:DesignHeight="300" d:DesignWidth="300">
    <Grid Background="#212121">
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="*" />
        </Grid.ColumnDefinitions>
        <Grid.RowDefinitions>
            <RowDefinition Height="100" />
            <RowDefinition Height="*" />
        </Grid.RowDefinitions>
        <contDateTime:ContDateTime Grid.Row="0" x:Name="ContDateTime"></contDateTime:ContDateTime>
        <contChart:ContChart Grid.Row="1" x:Name="ContChart"></contChart:ContChart>
    </Grid>
</UserControl>