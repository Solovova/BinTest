<Window x:Class="BinGuiDev.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:BinGuiDev"
        xmlns:dev="clr-namespace:BinGuiDev.Components.Dev"
        xmlns:components="clr-namespace:BinGuiDev.Components"
        xmlns:contDateTime="clr-namespace:BinGuiDev.Components.ContDateTime"
        mc:Ignorable="d"
        Title="MainWindow">


    <Grid Background="#212121">
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="*" />
        </Grid.ColumnDefinitions>
        <Grid.RowDefinitions>
            <RowDefinition Height="100" />
            <RowDefinition Height="*" />
        </Grid.RowDefinitions>
        <contDateTime:ContDateTime Grid.Row="0"></contDateTime:ContDateTime>
        <Button Grid.Row="1" Grid.Column="0" Click="OnToggleButtonClick">Тест</Button>
    </Grid>
</Window>