<Window x:Class="BinGuiDev.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:BinGuiDev"
        xmlns:dev="clr-namespace:BinGuiDev.Components.Dev"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">


    <Grid Background="#212121">
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="145" />
            <ColumnDefinition Width="*" />
            <ColumnDefinition Width="145" />
        </Grid.ColumnDefinitions>
        <Grid.RowDefinitions>
            <RowDefinition Height="100" />
            <RowDefinition Height="*" />
        </Grid.RowDefinitions>
        <dev:Dev3 Grid.Row="0" Grid.Column="0" x:Name="StartDateTime" />
        <dev:Dev3 Grid.Row="0" Grid.Column="2" x:Name="EndDateTime" />
        <Button Grid.Row="1" Grid.Column="0" Click="OnToggleButtonClick">Тест</Button>
    </Grid>
</Window>